*{margin:0;padding:0;}
img{border:none;}

#outer {
	height: 100%;
	overflow: hidden;
	position: relative;
	width: 100%;
	display:table;
}   
#outer[id] {   
    display: table;   
    position: static;   
    *position:relative;   
    _position: static;   
} 
  
